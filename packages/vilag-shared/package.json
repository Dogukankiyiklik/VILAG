{
    "name": "@vilag/shared",
    "version": "0.0.1",
    "description": "Shared types, constants and utilities for VILAG",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        },
        "./types": {
            "import": "./src/types/index.ts",
            "types": "./src/types/index.ts"
        },
        "./constants": {
            "import": "./src/constants/index.ts",
            "types": "./src/constants/index.ts"
        },
        "./utils": {
            "import": "./src/utils/index.ts",
            "types": "./src/utils/index.ts"
        }
    },
    "scripts": {
        "build": "tsc --noEmit",
        "typecheck": "tsc --noEmit"
    },
    "license": "Apache-2.0",
    "devDependencies": {
        "typescript": "^5.7.2"
    }
}